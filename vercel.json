{
  "functions": {
    "server.js": {
      "maxDuration": 10
    }
  },
  "headers": [
    {
      "source": "/socket.io/(.*)",
      "headers": [
        {
          "key": "X-Robots-Tag",
          "value": "noindex"
        }
      ]
    }
  ],
  "routes": [
    {
      "src": "/socket.io/(.*)",
      "status": 410,
      "headers": {
        "Cache-Control": "no-cache"
      }
    }
  ]
}